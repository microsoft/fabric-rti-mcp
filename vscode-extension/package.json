{
  "name": "fabric-mcp-client",
  "displayName": "Fabric MCP Client",
  "description": "Interface for Microsoft Fabric RTI MCP Server",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fabric-mcp.listEventstreams",
        "title": "List Eventstreams",
        "category": "Fabric MCP"
      },
      {
        "command": "fabric-mcp.queryKusto",
        "title": "Query Kusto",
        "category": "Fabric MCP"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "fabricMcpExplorer",
          "name": "Fabric MCP Tools",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.1.0"
  }
}
